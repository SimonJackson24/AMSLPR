[run]
source = src
omit =
    */venv/*
    */venv_test/*
    */__pycache__/*
    */tests/*
    */test_*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    if TYPE_CHECKING:

[html]
directory = htmlcov

[xml]
output = coverage.xml